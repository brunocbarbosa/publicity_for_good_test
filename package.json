{
  "name": "publicity_for_good_test_one",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dist": "tsup src --out-dir dist",
    "docker:compose" : "docker-compose up -d",
    "build": "run-s docker:compose prisma:migrate:deploy prisma:generate prisma:seed dist",
    "dev": "tsx watch src/server.ts",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:seed": "npx ts-node prisma/seed.ts",
    "test": "npx vitest run --dir src/tests/use-cases",
    "test:watch": "npx vitest --dir src/tests/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/tests/http",
    "test:e2e:watch": "vitest --dir src/tests/http"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/client": "^5.10.2",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^20.11.22",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@fastify/jwt": "8.0.0",
    "bcryptjs": "2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "pg": "8.11.3",
    "zod": "3.22.4"
  }
}
